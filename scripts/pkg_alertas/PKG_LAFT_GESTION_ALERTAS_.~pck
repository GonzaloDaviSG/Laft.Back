      INTO V_NIDADJUNTO
      FROM TBL_LAFT_ALERTA_PROCESO_ADJUN;
  
    INSERT INTO TBL_LAFT_ALERTA_PROCESO_ADJUN
      (NIDALERTA,
       NPERIODO_PROCESO,
       NIDADJUNTO,
       SRUTA_ADJUNTO,
